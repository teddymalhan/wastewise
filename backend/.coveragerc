[run]
source = .
omit = 
    */test/*
    */tests/*
    */__pycache__/*
    */venv/*
    */env/*
    */.venv/*
    */site-packages/*
    setup.py
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract
    @abstractmethod

precision = 2
show_missing = True

[html]
directory = htmlcov

[xml]
output = coverage.xml
